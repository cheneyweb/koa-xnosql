{
    "log": {
        "level": "warn"
    },
    "server": {
        "port": 3636,
        "xnosqlRoot": "/xnosql",
        "mongodbUrl": "mongodb://localhost:27017/test",
        "mongoOption": {
            "useUnifiedTopology": true,
            "useNewUrlParser": true
        },
        "mongodbName": "test",
        "defaultId": "_id",
        "defaultCreateAt": "createAt",
        "defaultUpdateAt": "updateAt",
        "defaultBlockBy": "blockBy",
        "defaultBlockByValue": "N",
        "defaultUTC": 8,
        "defaultSortBy": "createAt",
        "defaultSortOrder": -1,
        "defaultLimit": 200,
        "defaultLog": true
    }
}